# Module: virustotal
# Docs: https://www.elastic.co/guide/en/beats/filebeat/master/filebeat-module-virustotal.html

- module: virustotal
  # All logs
  livehunt:
    enabled: true
    # Set the VirusTotal private API key
    var.apikey: ""
    # Set retrieval limit, maximum 40, default 10
    var.limit: 10
